/**
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=notSel);

    Or(a=notSel, b=b, out=orNotSelB);
    Or(a=sel, b=a, out=orSelA);

    And(a=orNotSelB, b=orSelA, out=out);
}
